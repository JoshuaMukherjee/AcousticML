{
    "net":"MultiInputCNN",
    "net-args":{
        "CNN_args":{
            "layers":[48,48,48,48,2],
            "channels_in":2,
            "conv_args":{
                "kernel_size":3,
                "padding":"same"
            },
            "activation":"SELU"
        },
        "MLP_vector_args":{
            "layers":[4,32,32,32,32,512],
            "input_size":4,
            "activation":"SELU"
        },
        "MLP_feature_args":{
            "layers":[256,64,64,64,512],
            "input_size":1024,
            "activation":"SELU"
        }
    },
    "start-epochs":0,
    "epochs":50,
    "train":["GorkovTargetDatasetTrain-200000-4.pth"],
    "test":["GorkovTargetDatasetTest-1000-4.pth"],
    "optimiser":"NAdam",
    "optimiser-args":{"lr":1e-2},
    "loss-function":"gorkov_target_log_loss",
    "supervised":true,
    "batch":32,
    "solver":"wgs",
    "train-function":"train_gorkov_target_mCNN",
    "scheduler":"CyclicLR",
    "scheduler-args":{"base_lr":1e-2,
                        "max_lr":1e-1,
                        "step_size_up":10,
                        "cycle_momentum":false
                    }
}