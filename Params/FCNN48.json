{
    "net":"F_CNN",
    "net-args":{
        "CNN_args":{
            "layers":[32,512,128,64,2],
            "channels_in":4,
            "conv_args":{
                "kernel_size":3,
                "padding":"same"
            },
            "activation":"ReLU",
            "norm":"BatchNorm2d"
        }
    },
    "start-epochs":0,
    "epochs":50,
    "train":["FDatasetTrain-20000-4.pth"],
    "test":["FDatasetTest-1000-4.pth"],
    "optimiser":"NAdam",
    "optimiser-args":{"lr":1e-3},
    "loss-function":"AcousNetLoss",
    "supervised":true,
    "batch":2,
    "scheduler":"CyclicLR",
    "train-function":"train_FCNN_hologram",
    "scheduler-args":{"base_lr":1e-4,
                        "max_lr":1e-3,
                        "step_size_up":10,
                        "cycle_momentum":false
                    }
}