{
    "net":"MultiInputCNN",
    "net-args":{
        "CNN_args":{
            "layers":[32,64,64,64,64,64,64,64,64,64,64,64,64,2],
            "channels_in":2,
            "conv_args":{
                "kernel_size":3,
                "padding":"same"
            },
            "activation":"ReLU",
            "norm":"BatchNorm2d"
        },
        "MLP_vector_args":{
            "layers":[256,512],
            "input_size":4,
            "activation":"ReLU",
            "batch_norm":"BatchNorm1d"
        },
        "MLP_feature_args":{
            "layers":[512,512,512],
            "input_size":1024,
            "activation":"ReLU",
            "batch_norm":"BatchNorm1d"
        }
    },
    "start-epochs":0,
    "epochs":200,
    "train":["NaiveDatasetTrain-4-4.pth"],
    "test":["NaiveDatasetTest-2-4.pth"],
    "optimiser":"NAdam",
    "optimiser-args":{"lr":1e-3},
    "loss-function":"gorkov_FD_maxmean_loss",
    "supervised":true,
    "batch":2,
    "solver":"wgs",
    "train-function":"train_gorkov_no_target_mCNN"
}