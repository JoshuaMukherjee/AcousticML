{
    "net":"F_CNN",
    "net-args":{
        "CNN_args":{
            "layers":[32,1024,1024,512,512,64,32,2],
            "channels_in":4,
            "conv_args":{
                "kernel_size":3,
                "padding":"same"
            },
            "activation":"ReLU",
            "norm":"BatchNorm2d"
        }
    },
    "train-function":"train_FCNN_Slow",
    "start-epochs":0,
    "epochs":2000,
    "train":["FDatasetNormTrain-4-4.pth"],
    "test":["FDatasetNormTest-2-4.pth"],
    "optimiser":"NAdam",
    "optimiser-args":{"lr":1e-2},
    "loss-function":"mse_loss",
    "supervised":true,
    "batch":2,
    "scheduler":"CyclicLR",
    "scheduler-args":{"base_lr":1e-6,
                        "max_lr":1e-2,
                        "step_size_up":10,
                        "cycle_momentum":false
                    }
}